{
  "parser": "babel-eslint",
  "extends": "airbnb",
  "env": { "browser": true, "jest": true },
  "plugins": ["react", "jsx-ally", "import"],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "import/prefer-default-export": "off"
  },
  "settings": {
    "import/resolver": {
      "babel-plugin-root-import": {
        "rootPathSuffix": "src"
      }
    }
  }
}
